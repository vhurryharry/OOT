{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/inflector": "^1.3",
        "firebase/php-jwt": "^5.0",
        "ircmaxell/random-lib": "^1.2",
        "league/csv": "^9.4",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.26",
        "ramsey/uuid": "^3.8",
        "robmorgan/phinx": "^0.11.1",
        "sensio/framework-extra-bundle": "^5.4",
        "stripe/stripe-php": "^7.6",
        "symfony/asset": "^4.3",
        "symfony/cache": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/http-client": "^5.0",
        "symfony/mailer": "^4.3",
        "symfony/mailgun-mailer": "^4.3",
        "symfony/monolog-bundle": "^3.4",
        "symfony/security": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/templating": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/web-server-bundle": "^4.3",
        "symfony/yaml": "^4.3",
        "twig/extra-bundle": "^2.12",
        "twig/inky-extra": "^2.12"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.8",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.3",
        "sensiolabs/security-checker": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --verbose --show-progress=estimating",
        "lint:check": [
            "php-cs-fixer fix --dry-run --verbose --show-progress=estimating",
            "bin/console lint:yaml config"
        ],
        "phpunit": "phpunit",
        "phpstan": "phpstan analyze --debug -vvv --no-progress",
        "serve": "php bin/console server:run *:8080",
        "test": [
            "@lint:check",
            "@phpunit",
            "@phpstan"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.3"
        }
    }
}